<div class="task-group">
  <div class="task-name-container">
    <a name="/{{uid}}"></a>
    <a href="#/{{uid}}">
      <h3 class="section-name">{{name}}</h3>
    </a>
  </div>
  <ul class="task-group-list">
    {{#methods}}
    <li class="item symbol">
      <div class="task-group-term">
        <code>
        <a name="/{{htmlReferenceName}}">
        <a class="x-instance-method Swift" href="#/{{htmlReferenceName}}">
          {{#isInstanceMethod}}&ndash;&nbsp;{{/isInstanceMethod}}{{#isClassMethod}}+&nbsp;{{/isClassMethod}}{{#isProperty}}&nbsp;&nbsp;{{/isProperty}}{{methodSelector}}
        </a>
        </code>
      </div>
      <div class="height-container">
        <div class="pointy-thing-container"></div>
        <section class="section instance-method">
          <div class="pointy-thing"></div>
          {{#hasLongDescription}}
          <div class="abstract">
            {{{longDescription}}}
            {{#url}}
            <a href="{{{url}}}" class="see-more">See more</a>
            {{/url}}
          </div>
          {{/hasLongDescription}}
          {{#declaration}}
          <div class="declaration">
            <h4>Declaration</h4>

            <div class="Swift">
              <p class="aside-title">Swift</p>
              <p class="para">
              <code>{{declaration}}</code>
              </p>
            </div>
          </div>
          {{/declaration}}
          {{#hasMethodParameters}}
          <div class="parameters">
            <h4>Parameters</h4>
            <table class="graybox" border="0" cellspacing="0" cellpadding="5">
              <tbody>
                {{#methodParameters}}
                <tr>
                  <td scope="row">
                    <code>
                    <em class="term">{{argumentName}}</em>
                    </code>
                  </td>
                  <td>
                    <div class="definition">
                      {{{argumentDescription}}}
                    </div>
                  </td>
                </tr>
                {{/methodParameters}}
              </tbody>
            </table>
          </div>
          {{/hasMethodParameters}}
          {{#hasMethodResult}}
          <div class="result-description">
            <h4>Return Value</h4>
            {{{methodResult}}}
          </div>
          {{/hasMethodResult}}
          {{#github_token_url}}
          <div class="result-description view-on-github">
            <a href="{{{github_token_url}}}">Show on GitHub</a>
          </div>
          {{/github_token_url}}
        </section>
      </div>
    </li>
    {{/methods}}
  </ul>
</div>
